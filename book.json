{
    "root": "./",
    "title": "Bearo",
    "author": "Bearo",
    "description": "描述",
    "language": "en",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md",
        "summary": "SUMMARY.md"
    },
    "plugins": [
        "-sharing", "-lunr", "-search", 
        "search-pro",
        "back-to-top-button",
        "code",
        "-splitter",
        "anchor-navigation-ex",
        "terminal",
        "expandable-chapters"
    ],
    "pluginsConfig": {
        "atoc": {
			"addClass": true,
			"className": "atoc"
        },
        "code": {
            "copyButtons": true
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": false,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop":true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        }
    }
}